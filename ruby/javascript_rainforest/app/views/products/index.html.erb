<%= link_to 'New', new_product_path %>

<%= form_tag products_path, method: :get, authentication: false, id: 'search-form' do %>
  <%= text_field_tag :search, params[:search] %>
<% end %>

<h1>Products</h1>
<small id="pagination_top"><%= page_entries_info @products %></small>

<div id="products">
	<%= render @products %>
</div>

<%= paginate @products %>